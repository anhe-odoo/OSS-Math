{
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "description": "Extend Odoo spreadsheet features",
  "manifest_version": 3,
  "name": "OSS-Math",
  "host_permissions": ["<all_urls>"],
  "author": "Anthony Hendrickx",
  "version": "0.9",
  "web_accessible_resources": [
    {
      "resources": ["load_math.js", "spreadsheet_math.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
